{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "nsight compute test_kernels",
            "type": "shell",
            "command": "/usr/local/NVIDIA-Nsight-Compute/ncu",
            "args": [
                "-f",
                "-o", "test_matmul_8",
                "--import-source", "on",
                "--set", "full",
                "${workspaceFolder}/build/tests/test_kernels/test_kernels",
                "--gtest_filter=*design/test_matmul*"
            ],
            "dependsOrder": "sequence",
            "dependsOn": [
                "build release"
            ],
            "group": "test",
            "problemMatcher": []
        },
        {
            "label": "nsight systems test_network",
            "type": "shell",
            "command": "/usr/local/cuda/bin/nsys",
            "args": [
                "-f",
                "-o",
                "test_geam_tf",
                "--import-source",
                "on",
                "--set",
                "full",
                "${workspaceFolder}/build/tests/test_kernels/test_kernels",
                "--gtest_filter=design/test_geam.kgeam_smem_4xvec4_minbkcft/*"
            ],
            "dependsOrder": "sequence",
            "dependsOn": [
                "build release"
            ],
            "group": "test",
            "problemMatcher": []
        },
        {
            "label": "build release",
            "dependsOrder": "sequence",
            "dependsOn": [
                "configure release",
                "build"
            ],
            "group": "build"
        },
        {
            "label": "build debug",
            "dependsOrder": "sequence",
            "dependsOn": [
                "configure debug",
                "build"
            ],
            "group": "build"
        },
        {
            "label": "run test_kernels release",
            "type": "shell",
            "command": "build/tests/test_kernels/test_kernels",
            "args": [
                "--gtest_filter=*design/test_matmul*"
            ],
            "dependsOrder": "sequence",
            "dependsOn": [
                "configure release",
                "build"
            ],
            "group": "test"
        },
        {
            "label": "build",
            "type": "shell",
            "command": "cmake",
            "args": [
                "--build",
                "${workspaceFolder}/build",
                "--parallel"
            ],
            "group": "build"
        },
        {
            "label": "configure debug",
            "type": "shell",
            "command": "cmake",
            "args": [
                "-S",
                "${workspaceFolder}",
                "-B",
                "${workspaceFolder}/build",
                "-DCMAKE_BUILD_TYPE=debug",
                "-DCMAKE_VERBOSE_MAKEFILE=on",
                "-DDEBUG=1",
                "-DENABLE_PRECOMPILED_HEADERS=OFF"
            ],
            "group": "build"
        },
        {
            "label": "configure release",
            "type": "shell",
            "command": "cmake",
            "args": [
                "-S",
                "${workspaceFolder}",
                "-B",
                "${workspaceFolder}/build",
                "-DCMAKE_BUILD_TYPE=release",
                "-DCMAKE_VERBOSE_MAKEFILE=off"
            ],
            "group": "build"
        },
        {
            "label": "cmake clean",
            "type": "shell",
            "command": "cmake",
            "args": [
                "--build",
                "${workspaceFolder}/build",
                "--target",
                "clean"
            ],
            "group": "build"
        },
        {
            "label": "run release",
            "type": "shell",
            "command": "cmake",
            "args": [
                "--build",
                "${workspaceFolder}/build",
                "--target",
                "clean"
            ],
            "group": "build"
        }
    ]
  }
  